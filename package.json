  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "latest",
    "eslint": "latest",
    "jiti": "latest",
    "pkg-types": "^0.2.1",
    "siroc": "^0.16.0",
    "standard-version": "latest",
    "typescript": "latest",
    "unbuild": "latest"
  }
}
