{
  "name": "@vt7/tsconfig-module",
  "version": "0.0.1",
  "description": "Auto build tsconfig for nuxt module",
  "repository": "vuthanhbayit/tsconfig-module",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/tsConfig.js",
  "types": "./dist/tsConfig.d.ts",
  "files": [
